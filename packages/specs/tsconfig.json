{
  "extends": "@liquify/tsconfig",
  "exclude": [
    "node_modules/**",
    "package/**",
    "test"
  ],
  "include": [
    "tsup.config.ts",
    "scripts/tsup/tsup-enums.js",
    "src/**/*.ts",
    "node_modules/.cache/objects.ts",
    "scripts/*.ts"
   ],
  "compilerOptions": {
    "incremental": false,
    "allowJs": true,
    "removeComments": false,
    "typeRoots": [
      "node_modules/@types",
    ],
    "outDir": "package",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "html": [
        "./src/html/index.ts",
      ],
      "html/*": [
        "./src/html/controller/*.ts",
        "./src/html/*.ts",
        "!./src/html/index.ts"
      ],
      "html/data": [
        "./src/html/data/index.ts",
      ],
      "liquid": [
        "./src/liquid/index.ts",
      ],
      "liquid/*": [
        "./src/liquid/controller/*.ts",
        "./src/liquid/*.ts",
        "!./src/liquid/index.ts"
      ],
      "liquid/data": [
        "./src/liquid/data/index.ts",
      ],
      "utils/*": [
        "./src/utils/*.ts",
      ],
      "types/*": [
        "./src/types/*.ts",
      ],
      "tsup-plugin-enums": [
        "scripts/tsup/tsup-enums.js"
      ],
      "tsup-plugin-description": [
        "scripts/tsup/description.js"
      ]
    }
  }
}
